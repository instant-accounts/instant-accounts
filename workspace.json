{
  "version": 1,
  "projects": {
    "authorization-service": {
      "root": "apps/authorization-service",
      "sourceRoot": "apps/authorization-service/src",
      "projectType": "application",
      "prefix": "authorization-service",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/authorization-service",
            "main": "apps/authorization-service/src/main.ts",
            "tsConfig": "apps/authorization-service/tsconfig.app.json",
            "assets": [
              "apps/authorization-service/src/assets",
              "apps/authorization-service/src/config",
              {
                "glob": "**/*",
                "input": "libs/authorization-service/feature/oauth/src/proto",
                "output": "/proto"
              }
            ],
            "buildLibsFromSource": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/authorization-service/src/environments/environment.ts",
                  "with": "apps/authorization-service/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "port": 9410,
            "buildTarget": "authorization-service:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/authorization-service/.eslintrc",
            "tsConfig": ["apps/authorization-service/tsconfig.app.json", "apps/authorization-service/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/authorization-service/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/authorization-service/jest.config.js",
            "tsConfig": "apps/authorization-service/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "instant-accounts-web": {
      "root": "apps/instant-accounts-web",
      "sourceRoot": "apps/instant-accounts-web/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/instant-accounts-web",
            "index": "apps/instant-accounts-web/src/index.html",
            "main": "apps/instant-accounts-web/src/main.tsx",
            "polyfills": "apps/instant-accounts-web/src/polyfills.ts",
            "tsConfig": "apps/instant-accounts-web/tsconfig.app.json",
            "assets": ["apps/instant-accounts-web/src/favicon.ico", "apps/instant-accounts-web/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack",
            "buildLibsFromSource": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/instant-accounts-web/src/environments/environment.ts",
                  "with": "apps/instant-accounts-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "instant-accounts-web:build",
            "proxyConfig": "apps/instant-accounts-web/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "instant-accounts-web:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/instant-accounts-web/.eslintrc",
            "tsConfig": ["apps/instant-accounts-web/tsconfig.app.json", "apps/instant-accounts-web/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/instant-accounts-web/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/instant-accounts-web/jest.config.js",
            "tsConfig": "apps/instant-accounts-web/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "authorization-service-feature-oauth": {
      "root": "libs/authorization-service/feature/oauth",
      "sourceRoot": "libs/authorization-service/feature/oauth/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/authorization-service/feature/oauth/.eslintrc",
            "tsConfig": [
              "libs/authorization-service/feature/oauth/tsconfig.lib.json",
              "libs/authorization-service/feature/oauth/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/authorization-service/feature/oauth/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/authorization-service/feature/oauth/jest.config.js",
            "tsConfig": "libs/authorization-service/feature/oauth/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "application-api-gateway": {
      "root": "apps/application-api-gateway",
      "sourceRoot": "apps/application-api-gateway/src",
      "projectType": "application",
      "prefix": "application-api-gateway",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "webpackConfig": "apps/application-api-gateway/webpack.config.js",
            "outputPath": "dist/apps/application-api-gateway",
            "main": "apps/application-api-gateway/src/main.ts",
            "tsConfig": "apps/application-api-gateway/tsconfig.app.json",
            "assets": [
              "apps/application-api-gateway/src/assets",
              "apps/application-api-gateway/src/proto"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/application-api-gateway/src/environments/environment.ts",
                  "with": "apps/application-api-gateway/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "port": 9405,
            "buildTarget": "application-api-gateway:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/application-api-gateway/.eslintrc",
            "tsConfig": ["apps/application-api-gateway/tsconfig.app.json", "apps/application-api-gateway/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/application-api-gateway/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/application-api-gateway/jest.config.js",
            "tsConfig": "apps/application-api-gateway/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "public-api-gateway": {
      "root": "apps/public-api-gateway",
      "sourceRoot": "apps/public-api-gateway/src",
      "projectType": "application",
      "prefix": "public-api-gateway",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/public-api-gateway",
            "main": "apps/public-api-gateway/src/main.ts",
            "tsConfig": "apps/public-api-gateway/tsconfig.app.json",
            "assets": ["apps/public-api-gateway/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/public-api-gateway/src/environments/environment.ts",
                  "with": "apps/public-api-gateway/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "public-api-gateway:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/public-api-gateway/.eslintrc",
            "tsConfig": ["apps/public-api-gateway/tsconfig.app.json", "apps/public-api-gateway/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/public-api-gateway/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/public-api-gateway/jest.config.js",
            "tsConfig": "apps/public-api-gateway/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "authorization-service"
}
