{
  "version": 1,
  "projects": {
    "reserve-web": {
      "root": "apps/reserve-web",
      "sourceRoot": "apps/reserve-web/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/reserve-web",
            "index": "apps/reserve-web/src/index.html",
            "main": "apps/reserve-web/src/main.tsx",
            "polyfills": "apps/reserve-web/src/polyfills.ts",
            "tsConfig": "apps/reserve-web/tsconfig.app.json",
            "assets": [
              "apps/reserve-web/src/favicon.ico",
              "apps/reserve-web/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/reserve-web/src/environments/environment.ts",
                  "with": "apps/reserve-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "reserve-web:build",
            "proxyConfig": "apps/reserve-web/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "reserve-web:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/reserve-web/tsconfig.app.json",
              "apps/reserve-web/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/reserve-web/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/reserve-web/jest.config.js",
            "tsConfig": "apps/reserve-web/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "reserve-web-e2e": {
      "root": "apps/reserve-web-e2e",
      "sourceRoot": "apps/reserve-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/reserve-web-e2e/cypress.json",
            "tsConfig": "apps/reserve-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "reserve-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "reserve-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/reserve-web-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/reserve-web-e2e/**/*"]
          }
        }
      }
    },
    "reserve-public-api-gateway": {
      "root": "apps/reserve-public-api-gateway",
      "sourceRoot": "apps/reserve-public-api-gateway/src",
      "projectType": "application",
      "prefix": "reserve-public-api-gateway",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/reserve-public-api-gateway",
            "main": "apps/reserve-public-api-gateway/src/main.ts",
            "tsConfig": "apps/reserve-public-api-gateway/tsconfig.app.json",
            "assets": ["apps/reserve-public-api-gateway/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/reserve-public-api-gateway/src/environments/environment.ts",
                  "with": "apps/reserve-public-api-gateway/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "reserve-public-api-gateway:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/reserve-public-api-gateway/tsconfig.app.json",
              "apps/reserve-public-api-gateway/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/reserve-public-api-gateway/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/reserve-public-api-gateway/jest.config.js",
            "tsConfig": "apps/reserve-public-api-gateway/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "reserve-web"
}
